{
  "version": "1.1.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": {
      "include": [ "../Shared/**/*.cs" ]
    }
  },
  "dependencies": {
    "Microsoft.Extensions.CommandLineUtils": "1.1.0-*",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0-*"
        },
        "System.Console": "4.0.0-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    },
    "net451": {}
  },
  "commands": {
    "run": "LargeJsonApiClient"
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  }
}
